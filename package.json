{
  "name": "egg_js_boilerplate",
  "author": "Edgar_Huynh",
  "version": "2.0.0",
  "license": "MIT",
  "private": true,
  "homepage": "https://github.com/eggjs/egg",
  "bugs": {
    "url": "https://www.npmjs.com/package/egg"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eggjs/egg.git"
  },
  "egg": {
    "declarations": true,
    "typescript": true,
    "tscompiler": "ts-node/register/transpile-only"
  },
  "main": "index.js",
  "_moduleAliases": {
    "$": "./",
    "@": "./app",
    "@/utils": "./app/utils"
  },
  "scripts": {
    "preinstall": "npm install --package-lock-only --ignore-scripts --no-audit && npx npm-force-resolutions",
    "start": "egg-scripts start --title=v3s_vcash_server",
    "start-dev": "EGG_SERVER_ENV=dev egg-scripts start --title=v3s_vcash_server",
    "start-product": "EGG_SERVER_ENV=product egg-scripts start --title=v3s_vcash_server",
    "restart": "npm run stop && npm run start:product",
    "stop": "egg-scripts stop --title=egg-server-server",
    "dev": "npm run kill-port && cross-env NODE_ENV=DEV egg-bin --node-options--max-old-space-size=8192 dev",
    "product": "npm run kill-port && cross-env NODE_ENV=PROD egg-bin dev",
    "debug": "egg-bin debug",
    "compile": "npm run clean && tsc --build",
    "clean": "tsc --build --clean",
    "resolve": "npx npm-force-resolutions && npm install",
    "build": "npx ts-node --transpile-only app/utils/type/generator/build.ts",
    "lock": "npx ts-node --transpile-only app/utils/file/read-only/build.ts",
    "test": "npm run lint --fix && npm run test-local",
    "kill-port": "kill-port 7001 7002 7003",
    "test-local": "egg-bin --require module-alias/register test",
    "autod": "autod",
    "ci": "npm run lint && npm run cov",
    "cov": "egg-bin cov",
    "lint": "eslint --quiet --no-error-on-unmatched-pattern ."
  },
  "ci": {
    "version": "10"
  },
  "engines": {
    "node": ">=13.14.0",
    "npm": ">=6.14.4"
  },
  "resolutions": {
    "ajv": "^6.12.3",
    "ansi-regex": "^5.0.1",
    "engine.io": "^4.0.0",
    "follow-redirects": "1.14.8",
    "glob-parent": "^5.1.2",
    "handlebars": "^4.7.7",
    "hosted-git-info": "^4.0.2",
    "json-schema": "^0.4.0",
    "markdown-it": "^12.3.2",
    "mem": "^4.0.0",
    "minimist": "^1.2.3",
    "mongoose": "^6.2.5",
    "mpath": "^0.8.4",
    "nanoid": "^3.1.31",
    "path-parse": "^1.0.7",
    "set-value": "^4.0.1",
    "typescript": "~4.5.5",
    "urijs": "^1.19.8",
    "validator": "^13.7.0",
    "vm2": "^3.9.6",
    "y18n": "^5.0.8",
    "yargs-parser": "^18.1.2"
  },
  "dependencies": {
    "axios": "^0.21.4",
    "camelcase": "^5.3.1",
    "chalk": "^4.1.2",
    "cheerio": "^1.0.0-rc.10",
    "cross-env": "^7.0.3",
    "dotenv": "^10.0.0",
    "egg": "^2.30.0",
    "egg-core": "^4.20.0",
    "egg-cors": "^2.2.3",
    "egg-crypto": "^1.0.0",
    "egg-jwt": "^3.1.7",
    "egg-mailer": "^1.2.0",
    "egg-mongoose": "^3.3.1",
    "egg-passport": "^2.1.1",
    "egg-passport-github": "^1.0.0",
    "egg-passport-local": "^1.2.1",
    "egg-redis": "^2.4.0",
    "egg-scripts": "^2.13.0",
    "egg-sequelize": "^6.0.0",
    "egg-sequelize-auto": "^0.1.1",
    "egg-socket.io": "^4.1.6",
    "egg-userrole": "^2.1.0",
    "egg-validate": "^2.0.2",
    "exceljs": "^4.3.0",
    "googleapis": "^91.0.0",
    "handlebars": "^4.7.7",
    "js-ant": "^1.0.1",
    "lodash": "^4.17.21",
    "module-alias": "^2.2.2",
    "moment-timezone": "^0.5.33",
    "mongoose": "^6.2.5",
    "mysql2": "^2.3.3",
    "node-cache": "^5.1.2",
    "query-string": "^7.0.1",
    "request": "^2.88.2",
    "request-promise": "^4.2.6",
    "sequelize": "~6.6.5",
    "socket.io": "^4.4.1",
    "svg-captcha": "^1.4.0",
    "ua-parser-js": "^1.0.2",
    "validator": "^13.7.0"
  },
  "devDependencies": {
    "@types/chai": "^4.3.0",
    "@types/chai-subset": "^1.3.3",
    "@types/jsonwebtoken": "^8.5.6",
    "@types/koa-roles": "^2.0.0",
    "@types/mocha": "^9.0.0",
    "@types/nodemailer": "^6.4.4",
    "@types/ua-parser-js": "^0.7.36",
    "@typescript-eslint/eslint-plugin": "^4.31.0",
    "@typescript-eslint/parser": "^4.31.0",
    "autod": "^3.1.1",
    "autod-egg": "^1.1.0",
    "camelcase": "^5.3.1",
    "chai": "^4.3.4",
    "chai-subset": "^1.6.0",
    "co-mocha": "^1.2.2",
    "cross-env": "^7.0.3",
    "egg-bin": "^4.16.1",
    "egg-ci": "^1.18.0",
    "egg-mock": "^4.1.0",
    "eslint": "^7.25.0",
    "eslint-config-egg": "^9.0.0",
    "kill-port": "^1.6.1",
    "mocha": "^5.2.0",
    "npm-force-resolutions": "0.0.10",
    "ts-essentials": "^9.0.0",
    "ts-node": "^10.2.1",
    "typescript": "~4.5.5",
    "yarn": "^1.22.15"
  }
}
